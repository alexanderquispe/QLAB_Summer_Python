    "def calculator(x, y, intercept):\n",
    "    lnp1 = (1/x*x)\n",
    "    lnp2 = x*y\n",
    "    result = lnp1*lnp2\n",
    "    lnp3 = 1/x\n",
    "    lnp4 = y\n",
    "    result2 = lnp3 * lnp4\n",
    "    if (intercept == TRUE):\n",
    "        return result, lnp1, lnp2\n",
    "    elif(intercept == FALSE):\n",
    "        return result2, lnp3, lnp4\n",
    "    else:\n",
    "        print(\"Error, input does not meet the requirements\")"
